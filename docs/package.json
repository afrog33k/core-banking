{
	"name": "docs",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --port 9091 --host",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview --host --port 9091",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore --fix .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.68",
		"@sveltejs/kit": "^1.0.0-next.264",
		"@tailwindcss/typography": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"autoprefixer": "^10.4.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"postcss": "^8.4.6",
		"postcss-load-config": "^3.1.2",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.3",
		"svelte-preprocess": "^4.10.3",
		"tailwindcss": "^3.0.20",
		"tslib": "^2.3.1",
